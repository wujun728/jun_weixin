/* 智能排序 */
.typeSearch_Sort_container{
  width: 100%;
  height: 100%;
}
.typeSearch_Sort_container view{
  font-size: small;
  padding:10px;
  background: #fff;
  border-bottom: 1rpx solid #f1f1f1
}

/* 价格排序 */

.typeSearch_price_container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.typeSearch_price_container view{
  width: 90%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  color: #777;
  border-bottom: 1rpx solid #f1f1f1;
  font-size: small;
  padding: 5px 0;
}
.typeSearch_price_container input{
  width: 50%;
  height: 3em;
  color: #8B572A;
}

.typeSearch_price_container button{
  margin-top: 2em;
  width: 90%;
  background: #8B572A;
  color: #fff;
}

/* 商品排列样式1 */
.servant_page_container{
  background: #f1f1f1;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
  padding-bottom: 1em;
}
.servant_items{
  width: 44%;
  margin: 1%;
  padding: 2%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 10px;
  background: #fff;
  border-radius: 5px;
}
.servant_items image{
  width: 100%;
  height: 9em;
  border-radius: 5px;
}
.servant_items view{
  text-align: left;
  font-size: small;
  margin-top: 5px;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  width: 100%;
}
.servant_items text{
  font-size: small;
  width: 100%;
  text-align: left;
  margin-top: 5px;
}
.priceTagLine{
  color: #aaa;
  margin-left: 10px;
  text-decoration: line-through;
}
/* 没有商品时 */
.noservant{
  position: absolute;
  top: 170rpx;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: x-small;
}
.noservant image{
  width: 30%;
}
