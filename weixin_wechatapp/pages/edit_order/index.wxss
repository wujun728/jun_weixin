}
.input_telno{
  border:1rpx solid #ddd;
  width:38%;
  margin-right:20rpx;
  padding-left: 20rpx;
  font-size: 24rpx;
  border-radius:6rpx;
}
.addrItems > image{
  width: 30rpx;
  height: 30rpx;
  margin-right:36rpx;
}
.addrItems > .addrItems_info{
  padding: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
/* .addrItems_info{
} */
.addrItems_info image{
  width: 1.4em;
  height: 1.4em;

}
/*04.11修改*/
.addrItems_info view{
  margin-left: 1em;
  flex-direction: column;
  flex:1;

}
.addrItems_info view text{
  display: block;
}
.addrItems_info view text:nth-last-child(1){
  display: block;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
